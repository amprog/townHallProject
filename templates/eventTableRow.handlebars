<li id="{{eventId}}" class="event-row list-group-item " data-Main_State-filter={{State}} data-Member-filter={{Member}}
  data-container="body" data-animation="true" data-html="true" {{#if Notes}} data-toggle="popover"   title="Notes <a href='javascript:void(0)' class='close'>Ã—</a>"
  {{/if}}{{#unless Notes}} data-toggle="" {{/unless}}data-placement="top"
  data-content="{{#if Notes}}{{Notes}}{{/if}}{{#unless Notes}}There are no notes for this event.{{/unless}}">
  <span class="member">
    {{#if Member}}
    <a href="event.html?eid={{eventId}}" target="_self"><h4 class="event-name" data-value="{{Member}} ({{Party}}) {{State}}, {{District}}">
    {{Member}}
      <small>({{Party}})  {{State}},   {{District}}
      </small>
    </h4>
    </a>
    {{/if}}
    {{#unless Member}}
    <a href="event.html?eid={{eventId}}" target="_self"><h4 class="event-name" data-value="{{eventName}}">{{eventName}}</h4></a>
    {{/unless}}
 <span class="badge badge-default badge-pill pull-right badge-rsvpButton" style="margin-left: 20px; background-color: #ff4741;">RSVP</span>
 <a href="event.html?eid={{eventId}}" target="_self"><span class="badge badge-default badge-pill pull-right badge-details" style="margin-left: 20px;" data-eid="{{eventId}}">Details</span></a>
 <span class="badge badge-default badge-pill pull-right event-type" data-value="{{meetingType}}">  {{meetingType}}</span>
  </span>
  <ul class="list-inline list-inline-separated">
    {{#if Date}}
    <li class="event-date" data-value="{{Date}}">{{Date}}
    </li>
    {{/if}}
    {{#if Time}}
    <li class="event-time" data-value="{{Time}}">{{Time}}{{#if timeZone}}, {{timeZone}}{{/if}}  </li>
    {{/if}}
    {{#if Member}}
    {{#if eventName}}
    <li>{{eventName}}</li>
    {{/if}}
    {{/if}}
    {{#if Location}}
    <li class="event-venue" data-value="{{Location}}">{{Location}}</li>
    {{/if}}
    {{#if phoneNumber}}
    <li>{{phoneNumber}}</li>
    {{/if}}
    {{#if address}}
    <li class="event-address" data-value="{{address}}">{{address}}</li>
    {{/if}}
  </ul>
</li>
