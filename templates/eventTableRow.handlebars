<li id="{{eventId}}" class="event-row list-group-item " data-Main_State-filter={{State}} data-Member-filter={{Member}}
  data-container="body" data-animation="true" data-html="true" {{#if Notes}} data-toggle="popover"   title="Notes <a href='javascript:void(0)' class='close'>Ã—</a>"
  {{/if}}{{#unless Notes}} data-toggle="" {{/unless}}data-placement="top"
  data-content="{{#if Notes}}{{Notes}}{{/if}}{{#unless Notes}}There are no notes for this event.{{/unless}}">
  <span class="member">
    {{#if Member}}
    <h4>
    {{Member}}
      <small>({{Party}})  {{State}},   {{District}}
      </small>
    </h4>
    {{/if}}
    {{#unless Member}}
    <h4>{{eventName}}</h4>
    {{/unless}}
 <span class="badge badge-default badge-pill pull-right">  {{meetingType}}</span>
  </span>
  <ul class="list-inline list-inline-separated">
    {{#if Date}}
    <li>{{Date}}
    </li>
    {{/if}}
    {{#if Time}}
    <li>{{Time}}{{#if timeZone}}, {{timeZone}}{{/if}}  </li>
    {{/if}}
    {{#if Member}}
    {{#if eventName}}
    <li>{{eventName}}</li>
    {{/if}}
    {{/if}}
    {{#if Location}}
    <li>{{Location}}</li>
    {{/if}}
    {{#if phoneNumber}}
    <li>{{phoneNumber}}</li>
    {{/if}}
    {{#if address}}
    <li>{{address}}</li>
    {{/if}}
  </ul>
</li>
