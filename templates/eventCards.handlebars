<div class="col-md-12 col-sm-6">
  <div class="panel panel-secondary" id="{{eventId}}">
    <div class="panel-heading">
      <div class="row">
        <div class="col-xs-4 event-type" data-value="{{meetingType}}">
          <p>{{meetingType}}</p>
        </div>
        {{#if zoneString}}
        <div class="col-xs-8">
          <span class="addtocalendar pull-right">
            <a class="atcb-link btn btn-white btn-md"><span class="calendarButton">Add to Calendar </span><i class="fa fa-calendar-plus-o" aria-hidden="true"></i></a>
            <a class="btn btn-white btn-md rsvpButton"><span class="">RSVP</span></a>
            <a class="btn btn-white btn-md" href="event.html?eid={{eventId}}" target="_self"><span class="">Details</span></a>
            <var class="atc_event">
              <var class="atc_date_start">{{yearMonthDay}} {{timeStart24}}</var>
                <var class="atc_date_end">{{yearMonthDay}} {{timeEnd24}}</var>
                <var class="atc_timezone">{{zoneString}}</var>
                <var class="atc_title">{{meetingType}}: {{Member}}</var>
                <var class="atc_description">{{{Notes}}}</var>
                <var class="atc_location">{{address}}</var>
                <var class="atc_organizer">{{Party}}</var>
              </var>
          </span>
        </div>
      {{/if}}
      </div>
      <div class="row">
        <div class = col-xs-12>
          {{#if Member}}
        <h3>{{Member}} <small>({{Party}}, {{District}})</small></h3>
          {{/if}}
          {{#unless Member}}
          <h3 class="event-name" data-value="{{eventName}}">{{eventName}} </h3>
          {{/unless}}
      </div>
      </div>
    </div>
    <div class="panel-body">
      {{#if Member}}
      {{#if eventName}}
      <h3 class="text-success text-center">{{eventName}}</h3>
      {{/if}}
      {{/if}}
      <span class="text-success text-center event-date" data-value="{{Date}} {{Time}}"><h4>
        <span class>{{Date}}</span><span class="profile-summary-value"> at {{Time}}{{#if timeZone}}, {{timeZone}}{{/if}}</span></h4></span>
      <ul class="list-group list-group-flush">
        {{#if Notes}}
        <li class="list-group-item list-item-no-border">{{{Notes}}}
        </li>
        {{/if}}
        {{#if phoneNumber}}
        <li class="list-group-item list-item-no-border"> Call in number: {{phoneNumber}}
        </li>
        {{/if}}
        {{#if link}}
        <li class="list-group-item list-item-no-border event-link" data-value="{{link}}"">
          <a href="{{link}}">{{#if linkName}}linkName{{/if}}{{#unless linkName}}Find out more{{/unless}}</a>
        </li>
        {{/if}}
        {{#if RSVP}}
        <li class="list-group-item list-item-no-border">
          <a href="{{RSVP}}">{{rsvpName}}</a>
        </li>
        {{/if}}
        <li class="list-group-item">
          <address>
            <strong class="event-venue" data-value="{{Location}}">{{Location}}</strong></br>
            {{#if address}}
            <span class="event-address" data-value="{{address}}">{{address}}</span><br>
            {{/if}}
            {{#unless address}}
            {{streetAddress}}<br>
            {{#if City}}{{City}}, {{/if}}{{StateAb}} {{zip}}<br>
            {{/unless}}
            <a href={{addressLink}} target="_blank">Directions</a></br>
            <span class="text-center"><small>{{floatToFixed dist}} miles away from your search</small></span>
            <address>
          </li>
      </ul>
    </div>
  </div>
</div>
